<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  <connectionStrings>
    <add name="InventoryEntities" connectionString="#{InventoryEntities}" providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  </connectionStrings>
  <appSettings>
    <add key="LnEndPointAddress" value="#{LNEndpointAddress}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
</configuration>